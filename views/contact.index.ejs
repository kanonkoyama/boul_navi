<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>boul_navi</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <%- include('header'); %>
    <div class="main_wrapper">
      <div class="container">
        <ul class="contact">
        <% contacts.forEach((contact) => { %>
          <li>
            <p><a href=""><%= contact.name %></a></p>
            <p><%= nl2br(contact.content) %></p>
          </li>  
        <% })%>
        </ul>
      </div>
    </div>
    <footer>
    </footer>
  </body>
</html>