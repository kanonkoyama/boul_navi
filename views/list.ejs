<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>boul_navi</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <%- include('partials/header'); %>
    <div class="main_wrapper">
      <div class="container">
        <main>
          <% if(errors.length > 0){ %>
            <ul class="errors">
              <% errors.forEach(error => { %>
              <li><%= error %></li>
              <% }); %>
            </ul>
          <% } %>  
            <ul class="list">
            <% gims.forEach((gim) => { %>
            <li>
              <a href="/gim/<%= gim.id %>"><%= gim.name %></a>
              <img src="/images/sub_gim.<%= gim.id %>.jpg">
              <p>出典：<%= gim.website %></p>
              <p><%= gim.address %></p>
            </li>
            <% }) %>
          </ul>
        </main>
      </div>
    </div>
    <footer>
    </footer>
  </body>
</html>