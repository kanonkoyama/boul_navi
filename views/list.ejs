<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>boul_navi</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <%- include('header'); %>
    <div class="main_wrapper">
      <div class="container">
        <main>
          <ul class="list">
            <% gims.forEach((gim) => { %>
            <li>
              <a href="/gim/<%= gim.id %>"><%= gim.name %></a>
              <p><%= gim.address %></p>
            </li>
            <% }) %>
          </ul>
        </main>
      </div>
    </div>
    <footer>
    </footer>
  </body>
</html>