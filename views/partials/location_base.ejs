    <div id="map"></div>
    <script>
      var lat = document.getElementById( 'gimlocationlat' ).title;
      var lng = document.getElementById( 'gimlocationlng' ).title;      
      var map = L.map('map').setView([lat,lng], 13);
      L.tileLayer('http://{s}.tile.osm.org/{z}/{x}/{y}.png', {
        maxZoom: 18,
        attribution: 'Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors, '
      }).addTo(map);
     if(lat){
        console.log(lat);
      var marker2 = L.marker([lat,lng]).addTo(map);
      var popup = L.popup();
      function onMarker2Click(e) {
        popup
          .setLatLng(e.latlng)
          .setContent("<p>popup2</p><p>clickで表示されます。</p>")
          .openOn(map);
      }
      map.on('click', onMarker2Click);
      }

    </script>
